{
  "name": "Shopper",
  "version": "1.0",
  "description": "Travel from one site to another with a click",
  "browser_action":{
    "default_popup":"index.html",
    "default_icon":{
      "500":"images/icon.png"
    }
  },
  "permissions":["tabs"],
  "background":{
    "scripts":["background.js"],
    "persistent": false
  },
  "manifest_version": 2
}
